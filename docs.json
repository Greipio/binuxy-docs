{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Binuxy API Documentation",
  "colors": {
    "primary": "#A855F7",
    "light": "#A855F7",
    "dark": "#A855F7"
  },
  "contextual": {
    "options": ["chatgpt", "claude", "copy", "view"]
  },
  "favicon": "/favicon-1.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "about-binuxy",
              "api-policy",
              "authentication",
              "https-support",
              "error-codes",
              "usage-limits",
              "changelog"
            ]
          },
          {
            "group": "API Features",
            "pages": [
              "options/response-format",
              "options/development-environment"
            ]
          },
          {
            "group": "API Endpoints",
            "pages": ["api-reference/endpoint/bin"]
          }
        ]
      },
      {
        "tab": "Packages & Libraries",
        "groups": [
          {
            "group": "Packages & Libraries",
            "pages": [
              "packages-and-libraries/list-of-packages-and-libraries",
              "packages-and-libraries/other-resources"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Server Status",
          "href": "https://binuxy.openstatus.dev/",
          "icon": "signal-bars"
        },
        {
          "anchor": "FAQ",
          "href": "https://binuxy.com/faq",
          "icon": "circle-question"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "api": {
    "openapi": "https://greip.io/OpenAPI.json",
    "playground": {
      "display": "interactive"
    },
    "mdx": {
      "server": "https://api.binuxy.com",
      "auth": {
        "method": "bearer"
      }
    }
  },
  "appearance": {
    "default": "dark"
  },
  "background": {
    "image": "/images/background-light.png"
  },
  "navbar": {
    "links": [],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.binuxy.com/home"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/binuxy",
      "website": "https://binuxy.com"
    }
  },
  "redirects": []
}
